{
  "name": "app-website",
  "version": "0.0.21",
  "private": true,
  "author": "Jason Feng <solidzoro@live.com>",
  "license": "./LICENSE",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "-------------------------------- TOOLS ----": "----",
    "nuke": "rimraf build && rimraf yarn.lock && rimraf node_modules",
    "sync-dotenv-to-github-secrets": "node ./.github/scripts/sync-dotenv-to-github-secrets.js",
    "icons": "echo pls use `----pkg-uni` gen",
    "gencss": "cd ../----pkg-uni && pwd && yarn gencss",
    "-------------------------------- VERSION ----": "----",
    "__versionup:hotfix__": "e.g. yarn versionup:hotfix 2.71.2-f0",
    "versionup:hotfix": "standard-version -r --tag-prefix app-website@",
    "versionup": "standard-version -r patch --tag-prefix app-website@",
    "versionup:minor": "standard-version -r minor --tag-prefix app-website@",
    "versionup:major": "standard-version -r major --tag-prefix app-website@",
    "versionup:init": "standard-version --first-release --tag-prefix app-website@",
    "push-alltags": "git push --follow-tags",
    "-------------------------------- LINT ----": "----",
    "tsc-check": "tsc -v && tsc --noEmit",
    "lint-staged-check": "lint-staged",
    "lint": "eslint -v && eslint ./src",
    "fix": "eslint -v && eslint --fix ./src",
    "fix-eslint": "rimraf node_modules/.cache/.eslintcache && rimraf .eslintcache && npx yarn-deduplicate && yarn install",
    "fix-dependencies": "rnx-align-deps --write",
    "check-dependencies": "rnx-align-deps",
    "format": "yarn fix && prettier --write \"src/**/*.{ts,js,tsx,jsx,json,css,less,scss,sass}\"",
    "-------------------------------- DEV ----": "----",
    "dev": "rsbuild dev --open",
    "devs": "SSL=true rsbuild dev --open",
    "vvv": "RSDOCTOR=true DEBUG=rsbuild rsbuild dev --open",
    "build-vvv": "NODE_ENV=production RSDOCTOR=true GENERATE_SOURCEMAP=false rsbuild build",
    "-------------------------------- PROD ----": "----",
    "prebuild": "node ./scripts/utils/output-version-json-to-public-dir.js",
    "build": "GENERATE_SOURCEMAP=false rsbuild build",
    "preview": "NODE_ENV=production rsbuild preview",
    "build-preview": "yarn build && yarn preview",
    "inspect": "NODE_ENV=production rsbuild inspect --verbose",
    "-------------------------------- PREPROD ----": "----",
    "serve": "serve -s build",
    "serves": "serve -s build --ssl-cert scripts/ssl/cert.crt --ssl-key scripts/ssl/cert.key"
  },
  "updd": {
    "client": "yarn",
    "lockDeps": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "antd": "5.26.7"
    },
    "lockDevDeps": {
      "antd": "5.27+ 出现了 ConfigProvider 不生效问题"
    }
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
