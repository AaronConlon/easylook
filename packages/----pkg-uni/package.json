{
  "name": "----pkg-uni",
  "version": "0.0.2",
  "private": true,
  "author": "Jason Feng <solidzoro@live.com>",
  "scripts": {
    "-------------------------------- GEN ----": "----",
    "gen-uni-style-app": "tsx ./uni-styles/uni-style-app/__sh-gen-uni-style-app",
    "gen-uni-style-antd-media": "tsx ./uni-styles/uni-style-antd-media/__sh-gen-uni-style-antd-media",
    "gen-uni-style-antd-token": "tsx ./uni-styles/uni-style-antd-token/__sh-gen-uni-style-antd-token",
    "gencss": "yarn gen-uni-style-app && yarn gen-uni-style-antd-media && yarn gen-uni-style-antd-token",
    "-------------------------------- VERSION ----": "----",
    "versionup": "standard-version -r patch --skip.tag",
    "versionup:minor": "standard-version -r minor --skip.tag",
    "versionup:major": "standard-version -r major --skip.tag",
    "versionup:init": "standard-version --first-release --skip.tag",
    "push-alltags": "git push --follow-tags",
    "-------------------------------- TOOLS ----": "----",
    "icons": "iconfont-taro",
    "tsc-check": "tsc -v && tsc --noEmit"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "dependencies": {
    "@ant-design/static-style-extract": "*",
    "@asteasolutions/zod-to-openapi": "*",
    "antd": "*",
    "dayjs": "*",
    "copy-to-clipboard": "^3.3.3",
    "crypto-js": "^4.2.0",
    "http-status": "*",
    "prettier": "*",
    "react-use": "*",
    "typescript": "*",
    "url": "*",
    "zod": "*",
    "zustand": "*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "*",
    "@eslint/js": "*",
    "@tanstack/eslint-plugin-query": "*",
    "@types/crypto-js": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@typescript-eslint/utils": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-react-refresh": "*",
    "globals": "*",
    "jiti": "*",
    "react-iconfont-cli-mod-by-jasonfeng": "^2",
    "react-native-iconfont-cli-mod-by-jasonfeng": "^2",
    "taro-iconfont-cli-mod-by-jasonfeng": "^3.3.228",
    "tsx": "*",
    "typescript-eslint": "*"
  }
}
