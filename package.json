{
  "name": "app-website",
  "version": "0.1.11",
  "private": true,
  "author": "Jason Feng <solidzoro@live.com>",
  "license": "./LICENSE",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "-------------------------------- TOOLS ----": "----",
    "nuke": "rimraf build && rimraf yarn.lock && rimraf node_modules",
    "sync-dotenv-to-github-secrets": "node ../../.github/scripts/sync-dotenv-to-github-secrets.js",
    "icons": "echo pls use `----pkg-uni` gen",
    "gen-style-app": "tsx ./src/__shared__/styles/style-app/__sh-gen-style-app",
    "gen-style-antd-media": "tsx ./src/__shared__/styles/style-antd-media/__sh-gen-style-antd-media",
    "gencss": "yarn gen-style-app && yarn gen-style-antd-media",
    "-------------------------------- VERSION ----": "----",
    "__versionup:hotfix__": "e.g. yarn versionup:hotfix 2.71.2-f0",
    "versionup:hotfix": "standard-version -r --tag-prefix app-website@v",
    "versionup": "standard-version -r patch --tag-prefix app-website@v",
    "versionup:minor": "standard-version -r minor --tag-prefix app-website@v",
    "versionup:major": "standard-version -r major --tag-prefix app-website@v",
    "versionup:init": "standard-version --first-release --tag-prefix app-website@v",
    "push-alltags": "git push --follow-tags",
    "-------------------------------- LINT ----": "----",
    "tsc-check": "tsc -v && tsc --noEmit --strict",
    "lint-staged-check": "lint-staged",
    "lint": "eslint -v && eslint ./src",
    "fix": "eslint -v && eslint --fix ./src",
    "fix-eslint": "rimraf node_modules/.cache/.eslintcache && rimraf .eslintcache && npx yarn-deduplicate && yarn install",
    "fix-dependencies": "rnx-align-deps --write",
    "check-dependencies": "rnx-align-deps",
    "format": "yarn fix && prettier --write \"src/**/*.{ts,js,tsx,jsx,json,css,less,scss,sass}\"",
    "-------------------------------- DEV ----": "----",
    "dev": "rsbuild dev --open",
    "devs": "SSL=true rsbuild dev --open",
    "vvv": "RSDOCTOR=true DEBUG=rsbuild rsbuild dev --open",
    "build-vvv": "NODE_ENV=production RSDOCTOR=true GENERATE_SOURCEMAP=false rsbuild build",
    "-------------------------------- PROD ----": "----",
    "prebuild": "node ./scripts/utils/output-version-json-to-public-dir.js",
    "build": "GENERATE_SOURCEMAP=false rsbuild build",
    "preview": "NODE_ENV=production rsbuild preview",
    "build-preview": "yarn build && yarn preview",
    "inspect": "NODE_ENV=production rsbuild inspect --verbose",
    "vercel-install": "sh scripts/vercle/vercel-submodule-fetch.sh && yarn install",
    "-------------------------------- DEPLOY ----": "----",
    "move-build-to-build-backup": "node ./scripts/utils/move-build-to-build-backup.js",
    "move-build-index-html-to-version-html": "node ./scripts/utils/move-build-index-html-to-version-html.js",
    "show-cdn-version-html-url": "node ./scripts/utils/show-cdn-version-html-url.js",
    "sync-to-cdn": "NODE_ENV=production qiniu-cdn-sync --refresh-cdn",
    "all-in-cdn": "yarn versionup && yarn build && yarn move-build-index-html-to-version-html && yarn sync-to-cdn && yarn show-cdn-version-html-url && yarn move-build-to-build-backup",
    "-------------------------------- PREPROD ----": "----",
    "serve": "serve -s build",
    "serves": "serve -s build --ssl-cert scripts/ssl/cert.crt --ssl-key scripts/ssl/cert.key",
    "prepare": "husky"
  },
  "updd": {
    "client": "yarn",
    "lockDeps": {
      "react": "19.2.0",
      "react-dom": "19.2.0"
    },
    "lockDevDeps": {}
  },
  "resolutions": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "js-yaml": "^4.1.1"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@gsap/react": "^2.1.2",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.136.1",
    "@tanstack/react-router-devtools": "^1.136.1",
    "animated-scroll-to": "^2.3.2",
    "antd": "^5.28.1",
    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "copy-image-clipboard": "^2.1.2",
    "copy-to-clipboard": "^3.3.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "file-saver": "^2.0.5",
    "geojson": "^0.5.0",
    "globalthis": "^1.0.4",
    "gsap": "^3.13.0",
    "http-status": "^2.1.0",
    "js-cookie": "^3.0.5",
    "keepalive-for-react": "^5.0.2",
    "keepalive-for-react-router": "^3.0.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.1",
    "maplibre-gl": "^5.12.0",
    "motion": "^12.23.24",
    "nuqs": "^2.7.3",
    "plyr-react": "^5.3.0",
    "react": "19.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^10.0.0",
    "react-leaflet": "^5.0.0",
    "react-rough-notation": "^1.0.8",
    "react-use": "^17.6.0",
    "reflect-metadata": "^0.2.2",
    "swiper": "^12.0.3",
    "use-intl": "^4.5.3",
    "zod": "^4.1.12",
    "zod-openapi": "^5.4.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.3",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@hono/zod-openapi": "^1.1.4",
    "@rollup/plugin-replace": "^6.0.3",
    "@rsbuild/core": "^1.6.6",
    "@rsbuild/plugin-basic-ssl": "^1.2.1",
    "@rsbuild/plugin-check-syntax": "^1.6.0",
    "@rsbuild/plugin-eslint": "^1.1.2",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rsbuild/plugin-svgr": "^1.2.2",
    "@rsdoctor/rspack-plugin": "^1.3.9",
    "@rspack/cli": "^1.6.3",
    "@rspack/core": "^1.6.3",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/router-plugin": "^1.136.8",
    "@types/file-saver": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.21",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@types/three": "^0.181.0",
    "@typescript-eslint/utils": "^8.46.4",
    "babel-plugin-import": "^1.13.8",
    "code-inspector-plugin": "^1.2.10",
    "commitlint": "^20.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gen-buildinfo-webpack-plugin": "^1.7.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "postcss-color-hex-alpha": "^10.0.0",
    "postcss-color-mod-function": "^4.1.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "postcss-viewport-unit-fallback": "^1.0.1",
    "prettier": "^3.6.2",
    "qiniu-cdn-sync": "^1.1.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.1.0",
    "sass-embedded": "^1.93.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "url": "^0.11.4"
  }
}
